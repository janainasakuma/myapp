generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Faker {
  id Int @id @default(autoincrement())
  fullName String 
  birthDate DateTime @db.Date
  email String?
  avatar String?
  userName String @unique
  password String
  state String?
  bio String?
  phone String?
  status String?
  createdAt DateTime @default(now()) @db.Timestamp(3)
}

